<templateSet group="myGroup">
  <template name="cmt" value="/**&#10; * @description: $DESCRIPTION$&#10; * @author: $USER$&#10; * @date: $DATE$&#10;*/" shortcut="ENTER" description="文件注释" toReformat="false" toShortenFQNames="true">
    <variable name="DESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name=".toList" value=".collect(Collectors.toList())" description=".collect(Collectors.toList())" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name=".toSet" value=".collect(Collectors.toSet())" description=".collect(Collectors.toSet())" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name=".join" value=".collect(Collectors.joining(&quot;$END$&quot;))" description=".collect(Collectors.joining(&quot;$END$&quot;))" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name=".groupBy" value=".collect(Collectors.groupingBy(e -&gt; $END$))" description=".collect(Collectors.groupingBy(e -&gt; $END$))" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>